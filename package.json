{
  "name": "workverse",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev:api": "NEXUS_REFLECTION=true ts-node-dev --clear --respawn -r tsconfig-paths/register src/api ",
    "reflect": "NEXUS_REFLECTION=true ts-node src/api",
    "build": "yarn reflect && next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "next lint",
    "analyze": "ANALYZE=true yarn build",
    "format": "prettier --write '{pages,src}/**/*.{ts,tsx,css}'",
    "test:e2e": "playwright test",
    "prepare": "husky install",
    "storybook": "start-storybook -s ./public -p 3001",
    "storybook:build": "build-storybook -s ./public",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio"
  },

  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,json,md,html}": [
      "prettier --write"
    ]
  },
  "graphql": {
    "schema": "http://localhost:3000/api/graphql",
    "documents": "src/**/*.{gql,ts,tsx}"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
